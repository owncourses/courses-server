{
    "type": "project",
    "license": "proprietary",
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "league/flysystem-aws-s3-v3": "^1.0",
        "lexik/jwt-authentication-bundle": "^2.6",
        "nelmio/cors-bundle": "^1.5",
        "oneup/flysystem-bundle": "^3.4",
        "pixassociates/sortable-behavior-bundle": "^1.5",
        "ramsey/uuid-doctrine": "^1.6",
        "sensio/framework-extra-bundle": "^5.5",
        "sonata-project/admin-bundle": "^3.66",
        "sonata-project/doctrine-orm-admin-bundle": "^3.13",
        "sonata-project/formatter-bundle": "^4.1",
        "stof/doctrine-extensions-bundle": "^1.4",
        "swp/settings-bundle": "2.0.x",
        "symfony/amazon-mailer": "4.4.*",
        "symfony/asset": "4.4.*",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/expression-language": "4.4.*",
        "symfony/flex": "^1.2",
        "symfony/form": "4.4.*",
        "symfony/framework-bundle": "4.4.*",
        "symfony/google-mailer": "4.4.*",
        "symfony/http-client": "4.4.*",
        "symfony/mailer": "4.4.*",
        "symfony/monolog-bundle": "^3.1",
        "symfony/orm-pack": "*",
        "symfony/process": "4.4.*",
        "symfony/security-bundle": "4.4.*",
        "symfony/serializer": "4.4.*",
        "symfony/serializer-pack": "*",
        "symfony/translation": "4.4.*",
        "symfony/twig-bundle": "4.4.*",
        "symfony/validator": "4.4.*",
        "symfony/web-link": "4.4.*",
        "symfony/yaml": "4.4.*",
        "vich/uploader-bundle": "^1.11"
    },
    "require-dev": {
        "behat/mink": "1.7.x-dev",
        "behat/mink-goutte-driver": "^1.2",
        "behat/symfony2-extension": "^2.1",
        "behatch/contexts": "^3.3",
        "friends-of-behat/symfony-extension": "^2.0",
        "phpspec/phpspec": "^6.0",
        "phpstan/phpstan": "^0.11.2",
        "phpstan/phpstan-symfony": "^0.11.1",
        "roave/security-advisories": "dev-master",
        "symfony/debug-pack": "*",
        "symfony/maker-bundle": "^1.17",
        "symfony/profiler-pack": "*",
        "symfony/web-server-bundle": "4.2.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "compile": [
            "php bin/console doctrine:migrations:migrate",
            "php bin/console doctrine:ckeditor:instal"
        ],
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "ckeditor:install": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "symfony": {
            "allow-contrib": true,
            "require": "4.4.*"
        }
    }
}
